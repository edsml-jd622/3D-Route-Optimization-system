@article{VANBEVERN2020118,
	abstract = {One of the most fundamental results in combinatorial optimization is the polynomial-time 3/2-approximation algorithm for the metric traveling salesman problem. It was presented by Christofides in 1976 and is well known as ``the Christofides algorithm''. Recently, some authors started calling it ``Christofides-Serdyukov algorithm'', pointing out that it was published independently in the USSR in 1978. We provide some historic background on Serdyukov's findings and a translation of his article from Russian into English.
Одним из самых фундаментальных результатов в области комбинаторной оптимизации является полиномиальный 3/2-приближённый алгоритм для метрической задачи коммивояжёра. Он был представлен Никосом Кристофидесом в 1976г. и хорошо известен под названием <<алгоритм Кристофидеса>>. В последнее время некоторые авторы стали называть его <<алгоритмом Кристофидеса-Сердюкова>>, утверждая, что он был опубликован независимо в СССР в 1978г. В этой статье рассматривается исторический контекст появления результата А. И. Сердюкова и приводится перевод его статьи с русского на английский язык.},
	author = {Ren{\'e} {van Bevern} and Viktoriia A. Slugina},
	date-added = {2023-06-05 15:18:38 +0100},
	date-modified = {2023-06-05 15:18:38 +0100},
	doi = {https://doi.org/10.1016/j.hm.2020.04.003},
	issn = {0315-0860},
	journal = {Historia Mathematica},
	keywords = {Combinatorial optimization, Christofides algorithm, USSR, Novosibirsk Akademgorodok},
	pages = {118-127},
	title = {A historical note on the 3/2-approximation algorithm for the metric traveling salesman problem},
	url = {https://www.sciencedirect.com/science/article/pii/S0315086020300240},
	volume = {53},
	year = {2020},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S0315086020300240},
	bdsk-url-2 = {https://doi.org/10.1016/j.hm.2020.04.003}}

@article{branch_elimination,
	abstract = {Travelling salesman problem (TSP) has been widely studied for the classical closed loop variant but less attention has been paid to the open loop variant. Open loop solution has property of being also a spanning tree, although not necessarily the minimum spanning tree (MST). In this paper, we present a simple branch elimination algorithm that removes the branches from MST by cutting one link and then reconnecting the resulting subtrees via selected leaf nodes. The number of iterations equals to the number of branches (b) in the MST. Typically, b &lt;&lt; n where n is the number of nodes. With O-Mopsi and Dots datasets, the algorithm reaches gap of 1.69% and 0.61 %, respectively. The algorithm is suitable especially for educational purposes by showing the connection between MST and TSP, but it can also serve as a quick approximation for more complex metaheuristics whose efficiency relies on quality of the initial solution.},
	article-number = {177},
	author = {Fr{\"a}nti, Pasi and Nenonen, Teemu and Yuan, Mingchuan},
	date-added = {2023-06-05 12:08:54 +0100},
	date-modified = {2023-06-05 12:09:19 +0100},
	doi = {10.3390/app11010177},
	issn = {2076-3417},
	journal = {Applied Sciences},
	number = {1},
	title = {Converting MST to TSP Path by Branch Elimination},
	url = {https://www.mdpi.com/2076-3417/11/1/177},
	volume = {11},
	year = {2021},
	bdsk-url-1 = {https://www.mdpi.com/2076-3417/11/1/177},
